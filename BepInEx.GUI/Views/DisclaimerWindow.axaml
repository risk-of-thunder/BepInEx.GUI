<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:vm="using:BepInEx.GUI.ViewModels"
		xmlns:views="clr-namespace:BepInEx.GUI.Views"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:Class="BepInEx.GUI.Views.DisclaimerWindow"
		Title="One Time Only Disclaimer">
		
	<Grid Margin="10, 10, 10, 10">
		
		<TextBlock Grid.Row="0" Text="{Binding DisclaimerText}" FontSize="17" TextWrapping="Wrap"/>

	</Grid>
	
</Window>